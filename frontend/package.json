  "private": true,
  "license": "GPL-3.0",
  "author": "Edifice",
  "type": "module",
  "scripts": {
    "build": "pnpm run typecheck && vite build",
    "dev": "vite",
    "typecheck": "tsc -b --noEmit",
    "format": "prettier --write .",
    "lint": "eslint .",
    "pre-commit": "lint-staged",
    "prepare": "cd .. && husky frontend/.husky || true",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm run format",
      "pnpm run lint"
    ]
  },
  "dependencies": {
    "@edifice.io/bootstrap": "develop-pedago",
    "@edifice.io/client": "develop-pedago",
    "@edifice.io/community-client-rest": "file:../client/rest",
    "@edifice.io/react": "develop-pedago",
    "@react-spring/web": "^9.7.5",
    "@tanstack/react-query": "5.62.7",
    "@uidotdev/usehooks": "2.4.1",
    "clsx": "^2.1.1",
    "i18next": "23.8.1",
    "i18next-http-backend": "2.4.2",
    "ode-explorer": "develop-pedago",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "4.0.13",
    "react-hook-form": "^7.56.3",
    "react-i18next": "14.1.0",
    "react-router-dom": "^6.30.0",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@eslint/js": "^9.26.0",
    "@tanstack/react-query-devtools": "^5.75.6",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "^18.19.100",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "eslint": "^9.26.0",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "15.2.9",
    "msw": "^2.8.1",
    "nx": "19.6.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": "20"
  },
  "nx": {}
}